<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="BestMauiApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:AppPages="clr-namespace:MAUI.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="BestMauiApp">

    <TabBar>
        <Tab Title="MainPage">
            <ShellContent ContentTemplate="{DataTemplate AppPages:MainPage}" />
        </Tab>
        <Tab Title="Excercise Tracker">
            <ShellContent ContentTemplate="{DataTemplate AppPages:Excercisetracker}" />
        </Tab>
    </TabBar>

</Shell>
